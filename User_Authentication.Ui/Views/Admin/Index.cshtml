@model List<Microsoft.AspNetCore.Identity.IdentityUser>




<h1 class="mt-3">users</h1>

<table>
    <thead>
        <tr>
            <th>UserName</th>
            <th>DepartmentName</th>
            <th>Roles</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var user in Model)
        {
             <tr>
                 <td>@user.UserName</td>
                 
             </tr>
        }
    </tbody>
</table> 